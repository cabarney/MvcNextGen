@model Venue

<div class="form-group">
    <label class="col-sm-2 control-label" asp-for="Name"></label>
    <div class="col-sm-10">
        <input asp-for="Name" class="form-control" />
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label" asp-for="StreetAddress"></label>
    <div class="col-sm-10">
        <input asp-for="StreetAddress" class="form-control" />
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label" asp-for="City"></label>
    <div class="col-sm-10">
        <input asp-for="City" class="form-control" />
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label" asp-for="State"></label>
    <div class="col-sm-10">
        <input asp-for="State" class="form-control" />
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label" asp-for="Zip"></label>
    <div class="col-sm-10">
        <input asp-for="Zip" class="form-control"/>
    </div>
</div>
<div class="form-group">
    <label class="col-sm-2 control-label" asp-for="Directions"></label>
    <div class="col-sm-10">
        <textarea asp-for="Directions" class="form-control"></textarea>
    </div>
</div>